#compdef serienmover

_serienmover() {
 	_arguments \
 	    '1: :_directories' \
		'--help[Hilfeseite]' \
		'--version[Versionshinweis]' \
		'--seriesdir=:Serien Verzeichnis:_directories' \
		'--ignore-seriesinfo[ignoriere Informationen vom Seriesstore]' \
		'--no-autoprocess[Frage immer nach der Aktion]';
}
compdef _serienmover serienmover
